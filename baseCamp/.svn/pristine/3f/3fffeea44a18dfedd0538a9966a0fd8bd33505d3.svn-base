package com.twoyears.model.dto;

import java.util.Date;

public class RequestReplyDTO {
	
	private int rqrp_no;
	private int rqNo;
	private String userId;
	private String rqrp_content;
	private Date rqrp_date;
	
	public RequestReplyDTO(){}

	public RequestReplyDTO(int rqrp_no, int rqNo, String userId,
			String rqrp_content, Date rqrp_date) {
		super();
		this.rqrp_no = rqrp_no;
		this.rqNo = rqNo;
		this.userId = userId;
		this.rqrp_content = rqrp_content;
		this.rqrp_date = rqrp_date;
	}

	public int getRqrp_no() {
		return rqrp_no;
	}

	public void setRqrp_no(int rqrp_no) {
		this.rqrp_no = rqrp_no;
	}

	public int getRqNo() {
		return rqNo;
	}

	public void setRqNo(int rqNo) {
		this.rqNo = rqNo;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getRqrp_content() {
		return rqrp_content;
	}

	public void setRqrp_content(String rqrp_content) {
		this.rqrp_content = rqrp_content;
	}

	public Date getRqrp_date() {
		return rqrp_date;
	}

	public void setRqrp_date(Date rqrp_date) {
		this.rqrp_date = rqrp_date;
	}

	@Override
	public String toString() {
		return "RequestReplyDTO [rqrp_no=" + rqrp_no + ", rqNo=" + rqNo
				+ ", userId=" + userId + ", rqrp_content=" + rqrp_content
				+ ", rqrp_date=" + rqrp_date + "]";
	}
	
	

}
