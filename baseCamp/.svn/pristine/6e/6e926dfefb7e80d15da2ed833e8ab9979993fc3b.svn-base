package com.twoyears.model.dto;

public class RequestNoticeDTO {

	private int rqNo;
	private String userId;
	private String rqTitle;
	private String rqContent;
	private String rqCount;
	private String rqDate;
	private String rqLocation;
	public String getRqLocation() {
		return rqLocation;
	}

	public void setRqLocation(String rqLocation) {
		this.rqLocation = rqLocation;
	}

	public String getRqCount() {
		return rqCount;
	}

	public void setRqCount(String rqCount) {
		this.rqCount = rqCount;
	}

	public String getRqDate() {
		return rqDate;
	}

	public void setRqDate(String rqDate) {
		this.rqDate = rqDate;
	}

	public RequestNoticeDTO() {
	}



	public int getRqNo() {
		return rqNo;
	}

	public void setRqNo(int rqNo) {
		this.rqNo = rqNo;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getRqTitle() {
		return rqTitle;
	}

	public void setRqTitle(String rqTitle) {
		this.rqTitle = rqTitle;
	}

	public String getRqContent() {
		return rqContent;
	}

	public void setRqContent(String rqContent) {
		this.rqContent = rqContent;
	}

	@Override
	public String toString() {
		return "RequestNoticeDTO [rqNo=" + rqNo + ", userId=" + userId
				+ ", rqTitle=" + rqTitle + ", rqContent=" + rqContent
				+ ", rqCount=" + rqCount + ", rqDate=" + rqDate + "]";
	}

	

}
