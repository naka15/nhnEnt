<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="twoyears.givook">
	<select id="selectByLastdate" resultType="com.twoyears.model.dto.boardMainViewDTO">

	select * from (select rownum r, aa.* from (select noticedate, booksubject, bookpic 
	from notice n, books b 
	where n.noticeno = b.noticeno
	order by noticedate desc ) aa )
	where r &lt;= 7

	</select>
</mapper>